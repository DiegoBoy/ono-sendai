[global_config]
  title_transmit_bg_color = "#a40000"
  enabled_plugins = Logger, ActivityWatch, InactivityWatch
[keybindings]
  next_profile = <Primary><Alt>p
  split_horiz = <Primary><Alt>h
  split_vert = <Primary><Alt>v
[profiles]
  [[default]]
    background_color = "#000000"
    cursor_color = "#EEEEEE"
    foreground_color = "#EEEEEE"
    scrollback_infinite = True
    palette = "#000000:#EA4335:#34A853:#FBBC05:#4285F4:#A142F4:#24C1E0:#EEEEEE:#656565:#EA4335:#34A853:#FBBC05:#4285F4:#A142F4:#24C1E0:#FFFFFF"
  [[google_light]]
    background_color = "#FFFFFF"
    cursor_color = "#656565"
    foreground_color = "#656565"
    scrollback_infinite = True
    palette = "#EEEEEE:#EA4335:#34A853:#FBA000:#4285F4:#A142F4:#24C1E0:#000000:#656565:#EA4335:#34A853:#FBA000:#4285F4:#A142F4:#24C1E0:#FFFFFF"
  [[dracula]]
    background_color = "#282a36"
    cursor_color = "#94A3A5"
    foreground_color = "#94A3A5"
    scrollback_infinite = True
    palette = "#262626:#e356a7:#42e66c:#e4f34a:#9b6bdf:#e64747:#75d7ec:#efa554:#7a7a7a:#ff79c6:#50fa7b:#f1fa8c:#bd93f9:#ff5555:#8be9fd:#ffb86c"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
      size = 900, 600
    [[[child1]]]
      type = Terminal
      parent = window0
