[global_config]
  enabled_plugins = LaunchpadBugURLHandler, LaunchpadCodeURLHandler, APTURLHandler, CustomCommandsMenu, Logger, ActivityWatch, InactivityWatch
[keybindings]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
  [[CustomCommandsMenu]]
    [[[HttpServer]]]
      enabled = True
      name = HttpServer
      command = python -m SimpleHTTPServer 80
      position = 0
    [[[lol]]]
      enabled = True
      name = lol
      command = echo 123
      position = 1
